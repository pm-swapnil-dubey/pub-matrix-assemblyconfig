<joins>
    <join>
        <type>CAMPAIGN</type>
        <joinFields>
            <joinField>
                <name>cmpg_id</name>
                <joinKey>true</joinKey>
            </joinField>
            <joinField>
                <name>cmpg_dsp_id</name>
            </joinField>
            <joinField>
                <name>cmpg_real_time_flag</name>
            </joinField>
        </joinFields>
        <withDimensions>
            <name>campaign_id</name>
        </withDimensions>
    </join>
    <join>
        <type>WINNING_CAMPAIGN</type>
        <joinFields>
            <joinField>
                <name>wcmpg_id</name>
                <joinKey>true</joinKey>
            </joinField>
            <joinField>
                <name>wcmpg_dsp_id</name>
            </joinField>
            <joinField>
                <name>wcmpg_real_time_flag</name>
            </joinField>
        </joinFields>
        <withDimensions>
            <name>winning_campaign_id</name>
        </withDimensions>
    </join>
    <join>
        <type>COUNTRY</type>
        <joinFields>
            <joinField>
                <name>cntry_id</name>
            </joinField>
            <joinField>
                <name>cntry_code</name>
                <joinKey>true</joinKey>
            </joinField>
        </joinFields>
        <withDimensions>
            <name>country_code</name>
        </withDimensions>
    </join>
    <join>
        <type>DOMAIN</type>
        <joinFields>
            <joinField>
                <name>dmn_url</name>
                <joinKey>true</joinKey>
            </joinField>
            <joinField>
                <name>dmn_id</name>
            </joinField>
            <joinField>
                <name>dmn_adv_id</name>
            </joinField>
            <joinField>
                <name>dmn_adv_category_id</name>
            </joinField>
        </joinFields>
        <withDimensions>
            <name>domain_url</name>
        </withDimensions>
    </join>
    <join>
        <type>BUYER_DSP</type>
        <joinFields>
            <joinField>
                <name>bd_pubmatic_buyer_id</name>
            </joinField>
            <joinField>
                <name>bd_dsp_id</name>
                <joinKey>true</joinKey>
            </joinField>
            <joinField>
                <name>bd_dsp_buyer_id</name>
                <joinKey>true</joinKey>
            </joinField>
        </joinFields>
        <withDimensions>
            <name>cmpg_dsp_id</name>
            <name>atd_id</name>
        </withDimensions>
    </join>
    <join>
        <type>SITE</type>
        <joinFields>
            <joinField>
                <name>s_id</name>
                <joinKey>true</joinKey>
            </joinField>
            <joinField>
                <name>s_api_flag</name>
            </joinField>
        </joinFields>
        <withDimensions>
            <name>site_id</name>
        </withDimensions>
    </join>
    <join>
        <type>DEAL_META</type>
        <joinFields>
            <joinField>
                <name>j_deal_id</name>
                <joinKey>true</joinKey>
            </joinField>
            <joinField>
                <name>j_deal_meta_id</name>
            </joinField>
        </joinFields>
        <withDimensions>
            <name>deal_id</name>
        </withDimensions>
    </join>
</joins>
